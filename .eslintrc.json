{
  "env": {
    "es2023": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      {
        "js": "always"
      }
    ],
    "max-len": [
      "error",
      {
        "code": 100
      }
    ]
  }
}
